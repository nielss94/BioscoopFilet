        android:theme="@style/ThemeFilet">
        <activity android:name=".PresentationApplicationLogicLayer.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
