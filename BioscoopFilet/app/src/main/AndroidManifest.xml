        </activity>
        <activity android:name=".PresentationApplicationLogicLayer.InformationActivity" />
        <activity android:name=".PresentationApplicationLogicLayer.PTActivity" />
        <activity android:name=".PresentationApplicationLogicLayer.FeedbackActivity" />
        <activity android:name=".PresentationApplicationLogicLayer.ContactActivity" />
        <activity android:name=".PresentationApplicationLogicLayer.ReviewsActivity" />
        <activity android:name=".PresentationApplicationLogicLayer.FilmOverviewActivity" />
        <activity android:name=".PresentationApplicationLogicLayer.PaymentActivity" />
        <activity android:name=".PresentationApplicationLogicLayer.MyFiletActivity" />
        <activity android:name=".PresentationApplicationLogicLayer.BuyTicketsActivity" />
        <activity android:name=".PresentationApplicationLogicLayer.SeatSelectionActivity" />
        <activity android:name=".PresentationApplicationLogicLayer.FilmDetailAgendaActivity" />
